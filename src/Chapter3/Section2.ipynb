    "import matplotlib.pyplot as plt\n",
    "\n",
    "def step_function(x):\n",
    "    \"\"\"ステップ関数\"\"\"\n",
    "    return np.array(x > 0, dtype=int)\n",
    "\n",
    "x = np.arange(-5, 5, 0.1)\n",
    "y = step_function(x)\n",
    "plt.plot(x, y)\n",
    "plt.show()"
